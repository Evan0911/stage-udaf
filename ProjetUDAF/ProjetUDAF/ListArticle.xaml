<Window x:Class="ProjetUDAF.ListArticle"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ProjetUDAF"
        mc:Ignorable="d"
        Title="ListArticle" Height="450" Width="800">
    <Grid Background="#FF3FA1A6">
        <WrapPanel x:Name="WrpArticle" Margin="10,65,10,10" SizeChanged="WrpSizeChanged"/>
        <Button x:Name="BtnRetour" Content="Retour" HorizontalAlignment="Right" VerticalAlignment="Top" Width="75" Margin="707,10,10,0" Height="50" Click="BtnRetour_Click"/>
    </Grid>
</Window>
